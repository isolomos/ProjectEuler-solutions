#include <iostream>

using namespace std;

int main()
{
	int a = 1, b = 2, c = 3;
	for (a = 1; a < 332; a++)
	{
		for (b = 2; b < 499; b++)
		{
			for (c = 3; c < 997; c++)
			{
				if ((a + b + c) == 1000)
				{
					if ((a*a + b*b) == c*c)
					{
						cout << a*b*c << endl;
					}
				}
			}
		}
	}
}
